{
  "name": "linkedin-ai-reply",
  "description": "A Chrome extension that generates LinkedIn replies using AI.",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "build": "tsc && wxt build",
    "build:firefox": "wxt build -b firefox",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "compile": "tsc --noEmit",
    "postinstall": "wxt prepare"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.8",
    "@types/react-dom": "^18.3.0",
    "@wxt-dev/module-react": "^1.1.1",
    "init": "^0.1.2",
    "npx": "^10.2.2",
    "tailwindcss": "^3.4.12",
    "typescript": "^5.6.2",
    "wxt": "^0.19.9"
  }
}
